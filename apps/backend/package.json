{
  "name": "@agent-ui/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx convex dev",
    "generate": "bunx convex dev --once",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pull": "bun run scripts/sync-convex-data.ts",
    "validate:data": "bun run scripts/validate-convex-data.ts",
    "report:missing-install": "bun run scripts/report-missing-install.ts",
    "patch:component-fields": "bun run scripts/patch-component-fields.ts",
    "reindex:embeddings": "bun run --env-file .env.local scripts/reindex-component-embeddings.ts"
  },
  "dependencies": {
    "@convex-dev/migrations": "^0.3.1",
    "convex": "1.32.0"
  }
}
